{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~Roles/*": [
                "src/pages/Roles/*"
            ],
            "~hooks/*": [
                "src/hooks/*"
            ],
            "~misc/*": [
                "src/misc/*"
            ],
            "~contexts/*": [
                "src/contexts/*"
            ],
            "~__tests__/*": [
                "src/components/__tests__/*"
            ],
            "~utils/*": [
                "src/misc/*"
            ]
        }
    }
}